{
  "name": "ahq-production-planning",
  "version": "3.3.16",
  "description": "Set up a LabsKit application following best practices",
  "repository": {
    "type": "git",
    "url": "git://bitbucket.org/oliverwymantechssg/labskit-template.git"
  },
  "main": "server.js",
  "scripts": {
    "test": "npm run eslint && npm run integrationTest",
    "eslint": "eslint --ext js,vue .",
    "prettier:lint": "prettier --check \"**/*.{js,vue}\" ",
    "prettier": "prettier --list-different --write \"**/*.{js,vue}\" ",
    "start:client": "cross-env ENABLE_WEBPACK_DEV_SERVER=true NODE_OPTIONS=--openssl-legacy-provider webpack serve --config ./build/webpack.config.js --hot --host 0.0.0.0",
    "start:server": "cross-env ENABLE_WEBPACK_DEV_SERVER=true nodemon --config ./build/nodemon.json server.js",
    "start:client-win": "cross-env ENABLE_WEBPACK_DEV_SERVER=true NODE_OPTIONS=--openssl-legacy-provider webpack serve --config ./build/webpack.config.js  --lazy --host 0.0.0.0",
    "start:server-win": "cross-env ENABLE_WEBPACK_DEV_SERVER=true nodemon --config ./build/nodemon.json --legacy-watch server.js",
    "start:bundles": "cross-env LK__WEBPACK__ENABLE_REBUILD=false nodemon --config ./build/nodemon.json server.js",
    "unitTest": "npm run unitTest:client && npm run unitTest:server",
    "unitTest:client": "cross-env NODE_ENV=test mochapack --webpack-config ./build/webpack.config.test.js --require test/unit/client/setup.js \"test/unit/client/**/*.test.js\"",
    "unitTest:server": "cross-env NODE_ENV=test mocha --timeout 20000 -R spec \"./test/unit/server/**/*test.js\"",
    "integrationTest": "cross-env NODE_ENV=test mocha --timeout 10000 -R spec \"./test/integration/**/*.js\"",
    "e2eTest": "npm run webpack&& cross-env PORT=9002 LK__WEBPACK__ENABLE_REBUILD=false NODE_ENV=test nightwatch --config ./test/e2e/nightwatch.conf.js",
    "e2eTest-noWP": "cross-env PORT=9002 LK__WEBPACK__ENABLE_REBUILD=false NODE_ENV=test nightwatch --config ./test/e2e/nightwatch.conf.js",
    "cover": "npm run cover:unit && npm run cover:integration && npm run cover:report",
    "cover:unit": "nyc --silent npm run unitTest",
    "cover:integration": "nyc --no-clean --silent npm run integrationTest",
    "cover:report": "nyc report --reporter=lcov --reporter=text --report-dir=.nyc_output",
    "webpack": "webpack --config ./build/webpack.config.js",
    "webpack:production": "npm run webpack -- --mode production --env production",
    "webpack:analyze": "webpack --config ./build/webpack.config.js --mode production --env production --analyze",
    "webpack:appsvc": "npm run webpack -- --mode production",
    "preversion": "npm test",
    "version-bump": "npm version patch && git push && git push --tags",
    "//version-tag": "The 'version-tag' script makes use of a '--tier=<value>' parameter (release, staging, etc.). If not supplied, the value 'dev' is used.",
    "version-tag": "node -e \"const tier = process.env.npm_config_tier || 'dev'; require('child_process').execSync(`git tag ${tier}-${process.env.npm_package_version} && git push --tags`);\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier:lint && npm run eslint"
    }
  },
  "keywords": [
    "vue",
    "express"
  ],
  "author": {
    "name": "Oliver Wyman Labs",
    "email": "OWLabsSWATTeam@oliverwyman.com",
    "url": "http://http://labs.oliverwyman.com/"
  },
  "license": "MIT",
  "dependencies": {
    "@labskit/auth": "file:libs/labskit-auth",
    "@labskit/config": "file:libs/labskit-config",
    "@labskit/core": "file:libs/labskit-core",
    "@labskit/mongo": "file:libs/labskit-mongo",
    "applicationinsights": "2.9.1",
    "applicationinsights-native-metrics": "0.0.10",
    "connect-mongo": "^4.6.0",
    "core-js": "3.11.1",
    "express": "^4.21.2",
    "http-status-codes": "^1.3.0",
    "lodash": "4.17.21",
    "numeral": "2.0.4",
    "sequelize": "^6.37.7",
    "tedious": "^6.0.0",
    "uuid": "8.3.2",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@babel/polyfill": "7.12.1",
    "@babel/preset-env": "7.12.1",
    "@babel/preset-stage-2": "7.8.3",
    "@vue/compiler-sfc": "^3.1.0",
    "@vue/eslint-config-prettier": "6.0.0",
    "autoprefixer": "10.0.2",
    "axios": "^1.9.0",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.4.1",
    "babel-plugin-rewire": "1.2.0",
    "babel-preset-airbnb": "5.0.0",
    "chai": "4.2.0",
    "chai-shallow-deep-equal": "1.4.6",
    "cross-env": "7.0.2",
    "eslint": "7.13.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "6.15.0",
    "eslint-friendly-formatter": "4.0.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-vue": "7.1.0",
    "eslint-webpack-plugin": "2.3.0",
    "exports-loader": "1.1.1",
    "file-loader": "6.2.0",
    "glob": "7.1.6",
    "howler": "2.1.3",
    "husky": "^4.3.8",
    "imports-loader": "1.2.0",
    "jquery": "3.5.1",
    "jsbarcode": "^3.11.5",
    "json-loader": "0.5.7",
    "lodash-es": "4.17.21",
    "mini-css-extract-plugin": "1.3.1",
    "mocha": "^11.2.2",
    "mochapack": "^2.1.4",
    "nightwatch": "^3.12.1",
    "nodemon": "^3.1.10",
    "nyc": "15.1.0",
    "papaparse": "^5.5.2",
    "path-browserify": "^1.0.1",
    "prettier": "1.18.2",
    "proxyquire": "2.1.3",
    "selenium-server-standalone-jar": "3.141.59",
    "sinon": "9.2.1",
    "sinon-chai": "3.5.0",
    "style-loader": "^3.3.3",
    "supertest": "6.0.1",
    "url-loader": "4.1.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
